{
	"main": "./main.js",
	"activationEvents": [
		"onStartupFinished"
	],
	"contributes": {
		"configuration": {
			"title": "Script",
			"properties": {
				"script.path": {
					"type": "string",
					"scope": "resource",
					"default": "",
					"description": "Path directory.\nDefault is `${workspaceFolder}/.vscode`\nIf you have multiple workspace,Then it will select the 1st one."
				},
				"script.main": {
					"type": "string",
					"default": "script",
					"scope": "resource",
					"description": "Name of the main file that to run.You don't need to include `.js`\nDefault is script.js.\neg: index.js, main.c ..."
				}
			}
		}
	},
	"name": "script",
	"version": "0.0.1",
	"publisher": "Nur",
	"icon": "logo.png",
	"displayName": "Script",
	"description": "Run VSCode API as script.",
	"categories": [
		"Other"
	],
	"engines": {
		"vscode": "^1.51.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/nurmohammed840/VSC.ext/tree/master/script"
	}
}