{
    "activationEvents": [
        "*"
    ],
    "contributes": {
        "commands": [],
        "configuration": {
            "title": "Script",
            "properties": {
                "script.main": {
                    "type": "string",
                    "scope": "resource",
                    "default": ".vscode/main.js",
                    "description": "The primary entry point to your program."
                },
                "script.suggestCreateScript": {
                    "type": "boolean",
                    "default": true,
                    "description": "Suggest for creating script, If there isn't any script founded in workspace."
                }
            }
        }
    },
    "enableProposedApi": true,
    "name": "script",
    "version": "0.2.1",
    "publisher": "Nur",
    "license": "MIT",
    "displayName": "Script",
    "description": "Run VSCode API as script.",
    "main": "src/_start.js",
    "icon": "img/logo.png",
    "qna": "https://github.com/nurmohammed840/extension.vsix/issues",
    "dependencies": {
        "@types/vscode": "^1.55.0"
    },
    "devDependencies": {
        "@types/node": "^14.14.37",
        "typescript": "^4.2.4",
        "vsce": "^1.87.1"
    },
    "engines": {
        "vscode": "^1.51.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "nur",
        "script"
    ],
    "galleryBanner": {
        "color": "#000",
        "theme": "dark"
    },
    "scripts": {
        "uninstall": "node ./src/lifecycle",
        "vscode:uninstall": "node ./src/lifecycle",
        "vscode:prepublish": "tsc"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nurmohammed840/extension.vsix/tree/Script"
    },
    "bugs": {
